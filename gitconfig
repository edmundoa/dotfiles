  name = Edmundo Alvarez
  email = edmundo.alvarez@xing.com
[branch]
  autosetuprebase = always
[pull]
  rebase = true
[push]
  default = tracking
[color]
  status = auto
  branch = auto
  ui = auto
  interactive = auto
[alias]
  aa = add --all
  co = checkout
  dc = diff --cached
  lcu = !git-log-changes-upstream "$@"
  lcb = !git-log-changes-base "$@"
  satus = !sh -c 'gti status'
  stauts = !sh -c 'gti status'
[grep]
  lineNumber = true
[core]
  pager = less -X
